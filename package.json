{
  "name": "altcha-server",
  "version": "1.3.0",
  "description": "Production-ready ALTCHA server with Docker support",
  "keywords": [
    "altcha",
    "captcha",
    "proof-of-work",
    "spam-protection",
    "elysia",
    "bun"
  ],
  "license": "MIT",
  "author": "",
  "type": "module",
  "module": "src/index.js",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "docker:build": "docker build -t shroomlife/altcha-server:latest .",
    "docker:publish": "docker build -t shroomlife/altcha-server:1.3.0 . && docker push shroomlife/altcha-server:1.3.0",
    "docker:run": "docker run -p 8080:8080 --env-file .env shroomlife/altcha-server:latest",
    "start": "bun run src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "altcha-lib": "1.3.0",
    "chalk": "5.6.2",
    "elysia": "1.4.16"
  },
  "devDependencies": {
    "@types/bun": "1.3.2"
  },
  "engines": {
    "bun": ">=1.0.0"
  }
}